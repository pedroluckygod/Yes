--F3X API by NotaPlayer
--9/12/2025
local lp = game:GetService("Players").LocalPlayer
local rp = game:GetService("ReplicatedStorage")

--locals
local f3x = {}

function f3x.getf3x()
    local tool
    for _, v in pairs(lp:GetChildren()) do
    if v.Name:find("SyncAPI") then
    tool = v.Parent
     print("Sucess")
    end
end
for _, v in pairs(rp:GetChildren()) do
if v.Name:find("SyncAPI") then
      tool = v.Parent
   end
end

return tool
end 
-- functions


--f3x check


    if f3x.getf3x() ~= nil then
    print("Sucess!, You Have F3X \n by @N0taExploitsLOL")
    else
    print("you dont have a f3x")    
    end
--F3X functions
local remote = getf3x()



 function f3x.new(PartType,CFrame)
     remote:InvokeServer("CreatePart",PartType,CFrame,workspace)
 end
 function f3x.resize(Part,CFrame,Size)
     remote:InvokeServer("SyncResize",{{Part = Part,CFrame = CFrame,Size = Size --[[ size is vector.create]] }})
end

 return f3x
