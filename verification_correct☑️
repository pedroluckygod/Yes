 

local gui = Instance.new("ScreenGui",game.CoreGui)
local label = Instance.new("TextLabel")
label.Size = UDim2.new(1.0, 0, 1.2, 0)
label.Position = UDim2.new(0.0, 0, 0, 0)
label.BackgroundColor3 = Color3.new(0, 0, 0)
