function antiskid()
  local Workspace = game:GetService("Workspace")
local Lighting = game:GetService("Lighting")

-- Função para remover objetos específicos do Workspace
local function removeSpecificObjects()
    for _, object in ipairs(Workspace:GetDescendants()) do
        if object:IsA("Hint") or object:IsA("Message") or object:IsA("Sound") or object:IsA("Decal") then
            object:Destroy()
        end
    end
end

-- Função para remover Skybox do Lighting
local function removeSkyboxes()
    for _, object in ipairs(Lighting:GetChildren()) do
        if object:IsA("Skybox") then
            object:Destroy()
        end
    end
end

-- Chama as funções para remover os objetos específicos
removeSpecificObjects()
  
removeSkyboxes()
end
  
