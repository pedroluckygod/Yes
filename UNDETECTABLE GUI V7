function una()
  local function unanchorAllParts(parent)
    for _, part in ipairs(parent:GetDescendants()) do
        if part:IsA("BasePart") then
            part.Anchored = false
        end
    end
end

-- Desancorar todas as partes do Workspace
unanchorAllParts(game.Workspace)
end

function exp()
  local Workspace = game:GetService("Workspace")
local Explosion = Instance.new("Explosion")

-- Defina os limites da área onde as explosões podem ocorrer
local minX = -50
local maxX = 50
local minY = 10
local maxY = 50
local minZ = -50
local maxZ = 50

local function createRandomExplosion()
    local randomX = math.random(minX, maxX)
    local randomY = math.random(minY, maxY)
    local randomZ = math.random(minZ, maxZ)
    
    local explosion = Explosion:Clone()
    explosion.Position = Vector3.new(randomX, randomY, randomZ)
    explosion.Parent = Workspace
end

-- Crie uma explosão a cada 5 segundos

    createRandomExplosion()
    
end



