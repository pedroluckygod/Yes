    player.CharacterAdded:Connect(function(character)
        local backpack = player:WaitForChild("Backpack")
        local tool = game:GetService("InsertService"):LoadAsset(gearId):FindFirstChildOfClass("Tool")
        if tool then
            tool.Parent = backpack
        end
    end)
end)
