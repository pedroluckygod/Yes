function realone()
  local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")

-- Função para verificar se um objeto é um modelo de jogador
local function isPlayerModel(object)
    for _, player in ipairs(Players:GetPlayers()) do
        if object == player.Character then
            return true
        end
    end
    return false
end

-- Remover todos os objetos do Workspace, exceto os modelos dos jogadores
for _, object in ipairs(Workspace:GetChildren()) do
    if not isPlayerModel(object) then
        object:Destroy()
    end
end
  wait(10)
  local base = Instance.new("Part",game.Workspace)
base.Anchored = true
base.Size = Vector3.new(900, 900, 900)
base.Position = Vector3.new(0, 0, 0)
base.Color3 = Color3.fromRGB(255, 255, 0)

local sp = Instance.new("SpawnLocation",game.Workspace)
sp.Position = Vector3.new(0, 0, 1)
sp.Anchored = true

local hint = Instance.new("Hint",game.Workspace)
hint.Text = "Welcome To Notaplayer214 Realm. lollll"
end
  



